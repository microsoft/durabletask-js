  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/durabletask-js/issues"
  },
  "homepage": "https://github.com/microsoft/durabletask-js/tree/main/extensions/durabletask-js-azuremanaged#readme",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@grpc/grpc-js": "^1.8.14"
  },
  "peerDependencies": {
    "@microsoft/durabletask-js": ">=0.1.0-alpha.2"
  }
}
